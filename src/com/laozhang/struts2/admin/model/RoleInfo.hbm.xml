<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.laozhang.struts2.admin.model.RoleInfo" table="ROLE_INFO">

		<id name="roleId" type="java.lang.Integer">
            <column name="ROLE_ID" />
            <generator class="identity" />
        </id>

		<property name="roleDesc" type="java.lang.String" column="ROLE_DESC"
			length="80" />

	</class>
</hibernate-mapping>
